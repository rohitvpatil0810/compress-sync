# .env.example

# Environment Name
NODE_ENV=development

# TimeZone
TZ=UTC

# Server listen to this port
PORT=8080

# Log Directory
LOG_DIR=logs

# Base Url 
BASE_URL=http://localhost:8080

# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema

# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings

# You now need to adjust the connection URL to point to your own database.
DATABASE_URL="mongodb://USERNAME:PASSWORD@HOST:PORT/DATABASE"

CLOUDFLARE_S3_ACCESS_KEY_ID=your_access_key
CLOUDFLARE_S3_SECRET_ACCESS_KEY=your_secret
CLOUDFLARE_S3_CLIENT_ENDPOINT=your_endpoint
CLOUDFLARE_S3_BUCKET=your_bucket_name

# Redis connection url
REDIS_URL=redis://localhost:6379s